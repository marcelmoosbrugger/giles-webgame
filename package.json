{
  "name": "giles-webgame",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/mmsbrggr/giles-webgame.git",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --env=dev --hot --historyApiFallback",
    "clean": "rm -rf .psci_modules bower_components node_modules output",
    "reinstall": "npm run clean && npm run install:dependencies",
    "install:dependencies": "npm i --no-optional && bower install",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:prod": "webpack --env=prod --progress --profile --colors --optimize-minimize --define process.env.NODE_ENV=\"'production'\"",
    "repl:purs": "purs repl 'bower_components/purescript-*/src/**/*.purs' 'src/app/purs/**/*.purs'"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-localstorage": "^0.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "purs-loader": "^3.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
